script/source = "extends Control

var username = \"\"
var password

var created = false



func _on_login_button_down() -> void:
	if !created:
		username = $Username.text
		password = $Password.text.sha256_text()
		created = true
		$Login.text = \"Login\"
		$Username.text =\"\"
		$Password.text = \"\"
		print(\"Account created\")
	else:
		if $Username.text == username:
			if $Password.text.sha256_text() == password:
				print(\"Login sucess \")
			else : 
				print(\"Login fail\")
		else: 
				print(\"Login fail\")
				
"

